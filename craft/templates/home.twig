{% extends (craft.request.isAjax and not craft.request.isLivePreview) ? "_ajax-layout" : "_layout" %}

{% block content %}
    <div class="js-ajax-wrapper">
      {% for block in entry.body.level(1) %}
              {% include '_blocks/' ~ block.type.handle with {block: block} only %}
      {% endfor %}
    </div>
{% endblock %}
